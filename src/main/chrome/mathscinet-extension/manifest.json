{
  "name": "Add arXiv links to mathscinet",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Add arXiv links to mathscinet",
  "content_scripts": [
    {
      "matches": ["http://www.ams.org/mathscinet*"],
      "js": ["jquery-1.6.4.min.js", "mathscinet.js"]
    }
  ],
  "permissions": [
    "http://arxiv.org/",
    "http://export.arxiv.org/*",
    "http://ams.org/*"
  ]
}
